nextflow_process {

    name "Test Process GET_CAZYDB"
    script "modules/local/blast_databases.nf"
    process "GET_CAZYDB"

    test("Can download CAZYDB") {

        then {
            assert process.success
            assert process.trace.tasks().size() == 1
            assert snapshot(process.out).match()
            with(process.out) {
              // check if emitted output has been created
              assert cazydb.size() == 1
            }
        }

    }

}
